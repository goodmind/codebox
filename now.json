{
  "name": "prettier-now",
  "version": 2,
  "alias": [
    "prettier-now"
  ],
  "github": {
    "autoAlias": true,
    "enabled": true
  },
  "builds": [
    {
      "src": "src/www/package.json",
      "use": "@now/static-build"
    },
    { "src": "src/www/index.html", "use": "@now/static" },
    {
      "src": "src/api/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/wow.html",
      "dest": "/dist/www.html"
    },
    {
      "src": "/www.html",
      "dest": "/src/www/index.html"
    },
    {
      "src": "/api",
      "methods": [
        "POST"
      ],
      "dest": "/src/api/index.js"
    },
    {
      "src": "/users",
      "methods": [
        "POST"
      ],
      "dest": "/users-api.js"
    },
    {
      "src": "/users/(?<id>[^/]*)",
      "dest": "/users-api.js?id=$id"
    },
    {
      "src": "/legacy",
      "status": 404
    },
    {
      "src": "/redirect",
      "status": 301,
      "headers": {
        "Location": "https://zeit.co/",
        "cache-control": "s-maxage=1000"
      }
    }
  ]
}