{
  "name": "prettier-now",
  "version": 2,
  "alias": [
    "prettier-now"
  ],
  "github": {
    "autoAlias": true,
    "enabled": true
  },
  "builds": [
    {
      "src": "src/www/package.json",
      "use": "@now/next"
    },
    {
      "src": "src/api/index.js",
      "use": "@now/node",
      "config": { "maxLambdaSize": "30mb" }
    }
  ],
  "routes": [
    {
      "src": "/api",
      "methods": [
        "POST"
      ],
      "dest": "/src/api/index.js"
    },
    {
      "src": "/redirect",
      "status": 301,
      "headers": {
        "Location": "https://zeit.co/",
        "cache-control": "s-maxage=1000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/src/www/$1"
    }
  ]
}